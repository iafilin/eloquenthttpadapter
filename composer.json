{
    "name": "iafilin/eloquenthttpadapter",
    "description": "EloquentHttpAdapter is a Laravel package that allows you to work with RESTful API data using an Eloquent-like syntax with HttpModel abstract class, interfaces, and custom exceptions.",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Filin",
            "email": "iafilin@vk.com",
            "homepage": "https://iafilin.t.me"
        }
    ],
    "homepage": "https://github.com/iafilin/eloquenthttpadapter",
    "keywords": [
        "Laravel",
        "Eloquent",
        "HTTP",
        "API",
        "Adapter",
        "EloquentHttpAdapter",
        "HttpModel",
        "REST",
        "Interfaces",
        "Exceptions"
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/psr7": "^2.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Iafilin\\EloquentHttpAdapter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iafilin\\EloquentHttpAdapter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iafilin\\EloquentHttpAdapter\\EloquentHttpAdapterServiceProvider"
            ],
            "aliases": {
                "EloquentHttpAdapter": "Iafilin\\EloquentHttpAdapter\\Facades\\EloquentHttpAdapter"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
